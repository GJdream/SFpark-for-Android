<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout android:id="@+id/RelativeLayout00"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:gravity="center" android:background="#FFFFFF">
	  <ImageView android:src="@drawable/title" android:layout_width="fill_parent" android:layout_height="fill_parent" android:id="@+id/titleImageView1"></ImageView>
    <TextView android:id="@+id/textVersion" android:textColor="#000000"
              android:layout_width="wrap_content" android:text="VERSION NAME" android:layout_height="wrap_content"
              android:layout_centerHorizontal="true"
              android:singleLine="false"
              android:layout_alignBottom="@+id/titleImageView1"
              android:paddingBottom="80dp"
              android:textStyle="bold"></TextView>
    
</RelativeLayout>
